# .env.example - YouTube Web Automation Analysis Project
# Copy this file to .env and fill in your actual values

# ============================================================================
# YouTube Web Automation Analysis - Environment Configuration
# ============================================================================

# Application Settings
APP_NAME=YouTube-Web-Automation-Analysis
APP_VERSION=0.1.0
ENV=development
API_DEBUG=true

# API Configuration (FastAPI Server)
# ============================================================================
API_HOST=0.0.0.0
API_PORT=8000
API_PREFIX=/api/v1
API_CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# ============================================================================
# Database Configuration
# ============================================================================
# SQLite (Development)
DB_URL=sqlite:///./youtube_automation.db
# PostgreSQL (Production)
# DB_URL=postgresql+asyncpg://user:password@localhost:5432/youtube_automation
DB_ECHO=false
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=10

# ============================================================================
# Cache Configuration
# ============================================================================
CACHE_ROOT=../AI_LLM_projects/ai_warehouse/cache
AI_CACHE_ROOT=../AI_LLM_projects/ai_warehouse/cache
CACHE_REDIS_ENABLE=True
CACHE_REDIS_URL=redis://localhost:6379/1
CACHE_AUTO_CLEANUP=true
CACHE_MAX_CACHE_SIZE_GB=50
CACHE_MEMORY_TTL_MINUTES=60


# ============================================================================
# Logging Configuration
# ============================================================================
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
LOG_FILE_PATH=./logs/youtube_automation.log

# ============================================================================
# YouTube Data API v3 Configuration
# ============================================================================
YOUTUBE_API_KEY=AIzaSyCS6e-Yj0srSav4X20r3ACWd0LDJ_-p8tc

# Quota Management
YOUTUBE_DAILY_QUOTA_LIMIT=10000
YOUTUBE_QUOTA_WARNING_THRESHOLD=8000
# Rate Limiting
YOUTUBE_REQUESTS_PER_SECOND=10.0
YOUTUBE_BURST_CAPACITY=20
# Request Settings
YOUTUBE_MAX_RETRIES=3
YOUTUBE_REQUEST_TIMEOUT=30
YOUTUBE_BACKOFF_BASE=2.0
# Search Defaults
YOUTUBE_DEFAULT_SEARCH_RESULTS=10
YOUTUBE_MAX_SEARCH_RESULTS=50
YOUTUBE_DEFAULT_SEARCH_ORDER=relevance
# Comment Fetching
YOUTUBE_DEFAULT_COMMENT_COUNT=100
YOUTUBE_MAX_COMMENT_COUNT=1000
YOUTUBE_DEFAULT_COMMENT_ORDER=relevance
# Caching
YOUTUBE_ENABLE_RESPONSE_CACHE=true
YOUTUBE_CACHE_TTL_SECONDS=3600
# Features
YOUTUBE_ENABLE_ADAPTIVE_RATE_LIMITING=true
YOUTUBE_ENABLE_QUOTA_TRACKING=true
YOUTUBE_ENABLE_FALLBACK_SCRAPING=true

# ============================================================================
# Scraping Settings
# ============================================================================

# Browser Settings
SCRAPING_ENABLED=true
SCRAPING_BROWSER_TYPE=chromium
SCRAPING_USE_SELENIUM=true
SCRAPING_USE_PLAYWRIGHT=false
SCRAPING_HEADLESS=true

# Performance
SCRAPING_MAX_CONCURRENT_BROWSERS=3
SCRAPING_PAGE_TIMEOUT=30000
SCRAPING_NAVIGATION_TIMEOUT=30000

# Anti-Detection
SCRAPING_USE_STEALTH_MODE=true
SCRAPING_RANDOMIZE_USER_AGENT=true
SCRAPING_ENABLE_COOKIES=true
SCRAPING_TIMEOUT_SECONDS=30
SCRAPING_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Rate Limiting
SCRAPING_REQUESTS_PER_MINUTE=30
SCRAPING_MIN_DELAY_SECONDS=1.0
SCRAPING_MAX_DELAY_SECONDS=3.0
SCRAPING_REQUEST_DELAY_SECONDS=2.0

# Retry Logic
SCRAPING_MAX_RETRIES=3
SCRAPING_RETRY_DELAY_SECONDS=5.0

# Proxy Settings (Optional)
SCRAPING_ENABLE_PROXY=false
SCRAPING_PROXY_LIST=

# Caching
SCRAPING_CACHE_HTML=true
SCRAPING_CACHE_SCREENSHOTS=false


# ----------------------------------------------------------------------------
# Celery Configuration (NEW - Phase 6)
# ----------------------------------------------------------------------------
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0
CELERY_TASK_SERIALIZER=json
CELERY_RESULT_SERIALIZER=json
CELERY_WORKER_CONCURRENCY=4
CELERY_WORKER_PREFETCH_MULTIPLIER=4
CELERY_WORKER_MAX_TASKS_PER_CHILD=1000
CELERY_TASK_TRACK_STARTED=true
CELERY_TASK_TIME_LIMIT=3600
CELERY_TASK_SOFT_TIME_LIMIT=1800
CELERY_TASK_ACKS_LATE=true
CELERY_TASK_REJECT_ON_WORKER_LOST=true
CELERY_TASK_MAX_RETRIES=3
CELERY_TASK_DEFAULT_RETRY_DELAY=60
CELERY_TASK_RETRY_BACKOFF=true
CELERY_TASK_RETRY_BACKOFF_MAX=600
CELERY_TASK_DEFAULT_QUEUE=default
CELERY_TASK_DEFAULT_RATE_LIMIT=10/m
CELERY_RESULT_EXPIRES=86400
CELERY_RESULT_PERSISTENT=true
CELERY_ENABLE_FLOWER=true
CELERY_FLOWER_PORT=5555
CELERY_FLOWER_BASIC_AUTH=
CELERY_BEAT_SCHEDULER=celery.beat:PersistentScheduler
CELERY_BEAT_SCHEDULE_FILENAME=celerybeat-schedule
CELERY_WORKER_HIJACK_ROOT_LOGGER=false

# ----------------------------------------------------------------------------
# Analysis Settings
# ----------------------------------------------------------------------------
ANALYSIS_ENABLE_SENTIMENT=true
ANALYSIS_ENABLE_TOPIC_MODELING=true
ANALYSIS_ENABLE_TRANSCRIPT=false
ANALYSIS_LANGUAGE_DETECTION=true
ANALYSIS_BATCH_SIZE=32
ANALYSIS_MIN_COMMENT_LENGTH=10
ANALYSIS_MAX_COMMENTS_PER_VIDEO=1000

# ----------------------------------------------------------------------------
# Storage Settings
# ----------------------------------------------------------------------------
STORAGE_OUTPUT_FORMAT=json
STORAGE_ENABLE_COMPRESSION=true
STORAGE_RETENTION_DAYS=30
STORAGE_AUTO_BACKUP=true
STORAGE_BACKUP_INTERVAL_HOURS=24

# ----------------------------------------------------------------------------
# Security Settings
# ----------------------------------------------------------------------------
SECURITY_RATE_LIMIT_PER_MINUTE=60
SECURITY_RATE_LIMIT_PER_HOUR=1000
SECURITY_ENABLE_CORS=true

# ----------------------------------------------------------------------------
# Logging Configuration
# ----------------------------------------------------------------------------
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
LOG_FILE_PATH=./logs/youtube_automation.log

# ----------------------------------------------------------------------------
# Feature Flags
# ----------------------------------------------------------------------------
FEATURE_ENABLE_CAPTION=true
FEATURE_ENABLE_VQA=true
FEATURE_ENABLE_CHAT=true
FEATURE_ENABLE_RAG=true
FEATURE_ENABLE_AGENT=true
FEATURE_ENABLE_GAME=true
FEATURE_ENABLE_T2I=true
FEATURE_ENABLE_EXPORT=true
FEATURE_ENABLE_TRAINING=true
FEATURE_ENABLE_MONITORING=true
FEATURE_PRELOAD_MODELS=false

# ----------------------------------------------------------------------------
# Development Settings
# ----------------------------------------------------------------------------
RELOAD_ON_CHANGE=true
DEBUG_SQL=false
ENABLE_PROFILING=false